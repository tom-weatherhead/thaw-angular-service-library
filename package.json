{
	"name": "thaw-angular-service-library",
	"version": "0.0.1",
	"description": "Services for Angular applications",
	"keywords": [
		"Angular",
		"TypeScript"
	],
	"private": false,
	"homepage": "https://github.com/tom-weatherhead/thaw-angular-service-library#readme",
	"repository": "github:tom-weatherhead/thaw-angular-service-library",
	"bugs": {
		"url": "https://github.com/tom-weatherhead/thaw-angular-service-library/issues"
	},
	"// browser": "",
	"// main": "",
	"main": "main.js",
	"// types": "",
	"// files": [],
	"man": [],
	"directories": {
		"lib": "lib",
		"// bin": "It is an error to specify both bin and directories.bin",
		"man": "man",
		"// doc": "Put markdown files in the directories.doc directory",
		"doc": "doc",
		"example": "examples",
		"test": "test"
	},
	"scripts": {
		"clean-packages": "rm -rf node_modules && rm -f package-lock.json",
		"clean-build-products": "rm -rf coverage && rm -rf dist",
		"clean": "npm run clean-packages && npm run clean-build-products",
		"build:dev": "npm run clean-build-products && ng build -c dev --base-href ./ && npm run build-electron-main",
		"build:prod": "npm run clean-build-products && ng build -c production --base-href ./ && npm run build-electron-main",
		"// post-build": "cp node_modules/bootstrap/dist/js/bootstrap.bundle.min.js.map dist/",
		"build-electron-main": "rm -f main.js; tsc -p tsconfig.main.json",
		"electron-with-gpu": "electron .",
		"electron-without-gpu": "electron --disable-gpu .",
		"electron": "npm run electron-without-gpu",
		"lint": "eslint . --ext .ts",
		"ng-lint": "ng lint",
		"prettiercheck": "prettier --check \"**/*.js\" \"**/*.ts\"",
		"prettierlist": "prettier --list-different \"**/*.js\" \"**/*.ts\"",
		"format": "prettier --write \"**/*.js\" \"**/*.ts\"",
		"unittests-headless": "ng test --watch=false",
		"e2e": "cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' jasmine-ts --config=e2e/jasmine.json",
		"pre-build-tests": "npm run prettiercheck && npm run lint && npm run ng-lint",
		"test": "npm run unittests-headless && npm run e2e",
		"build-dev-and-test": "npm run pre-build-tests && npm run build:dev && npm test",
		"build-prod-and-test": "npm run pre-build-tests && npm run build:prod && npm test",
		"travis": "npm i && npm run build-prod-and-test",
		"all": "npm run clean && npm run travis && npm run pack-and-deploy",
		"packagr": "ng-packagr -p ng-package.json",
		"packit": "npm run packagr && cd dist && npm pack && cd ..",
		"deploy": "eval mv dist/$npm_package_name-$npm_package_version.tgz $(echo $npm_package_config_deploy_target_dir)/",
		"pack-and-deploy": "ng build --prod && npm run packit && npm run deploy",
		"pack-and-publish": "ng build --prod && npm run packagr && npm publish dist",
		"start": "npm run all",
		"gen-module": "ng generate module",
		"gen-component": "ng generate component",
		"gen-service": "ng generate service",
		"// e.g. gen": "npm run gen-module modules/price-macd-signal-charts-canvases; npm run gen-component modules/price-macd-signal-charts-canvases",
		"loc": "git ls-files | grep -v package-lock.json | tr -d '\\r' | tr '\\n' '\\0' | xargs -0 wc -l",
		"re": "npm run format && npm run build-dev-and-test && npm run pack-and-deploy",
		"f": "npm run format && npm run all",
		"e": "electron ."
	},
	"config": {
		"deploy_target_dir": "$NPM_PKG_DIR"
	},
	"publishConfig": {},
	"dependencies": {
		"@angular/common": "^10.2.3",
		"@angular/compiler": "^10.2.3",
		"@angular/core": "^10.2.3",
		"@angular/forms": "^10.2.3",
		"@angular/localize": "^10.2.3",
		"@angular/platform-browser": "^10.2.3",
		"@angular/platform-browser-dynamic": "^10.2.3",
		"@angular/router": "^10.2.3",
		"rxjs": "^6.6.3",
		"thaw-common-utilities.ts": "^0.0.2",
		"tslib": "^2.0.3",
		"zone.js": "~0.10.3"
	},
	"devDependencies": {
		"@angular-devkit/architect": "~0.1001.4",
		"@angular-devkit/build-angular": "^0.1002.0",
		"@angular-eslint/builder": "^0.6.0-beta.0",
		"@angular-eslint/eslint-plugin": "^0.6.0-beta.0",
		"@angular-eslint/eslint-plugin-template": "^0.6.0-beta.0",
		"@angular-eslint/template-parser": "^0.6.0-beta.0",
		"@angular/cli": "^10.2.0",
		"@angular/compiler-cli": "^10.2.3",
		"@angular/language-service": "^10.2.3",
		"@types/chai": "^4.2.14",
		"@types/eslint": "^7.2.4",
		"@types/jasmine": "~3.6.1",
		"@types/jasminewd2": "~2.0.8",
		"@types/jquery": "^3.5.4",
		"@types/node": "^14",
		"@typescript-eslint/eslint-plugin": "^4.7.0",
		"@typescript-eslint/parser": "^4.7.0",
		"chai": "^4.2.0",
		"cross-env": "^7.0.2",
		"electron": "^10.1.5",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-rxjs": "^2.1.3",
		"jasmine": "~3.6.3",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~6.0.0",
		"jasmine-ts": "~0.3.0",
		"karma": "~5.2.3",
		"karma-coverage-istanbul-reporter": "~3.0.3",
		"karma-electron": "^6.3.1",
		"karma-jasmine": "~4.0.1",
		"karma-jasmine-html-reporter": "^1.5.4",
		"ng-packagr": "^10.1.2",
		"prettier": "^2.1.2",
		"rollup": "^2.33.1",
		"spectron": "^12.0.0",
		"ts-node": "<8",
		"typescript": "~3.9"
	},
	"// bundledDependencies": "... is an array of package names",
	"engines": {
		"node": "^14"
	},
